<h1>Users</h1>

<table class="table table-hover">
<thead>
    <tr>
    <th>Name</th>
    <th>Email</th>
    <th>User id</th>
    </tr>
</thead>

<tbody>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.id %></td>
        </tr>
    <% end %>
</tbody>
</table>
<br>